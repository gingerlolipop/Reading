   journal = {Scandinavian Journal of Urology},
   keywords = {Adverse effects,Norwegian EPIC-26,prostate cancer,reliability,validity},
   note = {Norwegian version<br/><br/>Sample size: 471<br/><br/>Reliability and validity <br/><br/>four domains- 2 subdomains<br/><br/>Trash},
   pages = {280-285},
   title = {Psychometric testing of the Norwegian version of the Expanded Prostate Cancer Index Composite 26-item version (EPIC-26)},
