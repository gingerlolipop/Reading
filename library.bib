   keywords = {botulinum neurotoxin type A,myelodysplasia,nerve growth factor,tissue inhibitor of matrix metalloproteinase 2,transforming growth factor beta 1},
   note = {NGF levels were suggested to have a role in evaluating post-BoNT-A therapy success as it helped to predict response in the study of Liu et al},
   pages = {1896-1902},
   title = {The effect of intradetrusor botulinum neurotoxin type A on urinary NGF, TGF BETA-1, TIMP-2 levels in children with neurogenic detrusor overactivity due to myelodysplasia},
