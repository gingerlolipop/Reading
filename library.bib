@article{Kimura2017,
   author = {Tetsuo Kimura},
   journal = {Journal of Educational Evaluation for Health Professions},
   keywords = {computer adaptive testing,content balance,learning self-efficacy,measurement efficiency,motivation},
   note = {Limitations of CAT:<br/>1. Coverage and balance of subject matter content.<br/><br/>(1) It is imposible to review the items presented on CATs in advance because CATs select items based on test-takers' responses. <br/><br/>(2) If a CAT selects items solely based on the test-takers' ability, content balance and coverage may be easily distorted for some test-takers. There are also other nonstatistical specifications for CAT that we must consider, such as item format, answer keys, test length, and item enemies, which are pairs or clusters of items that are generally prohibited from appearing together on the same examination, since they could cue one another, or are too similar or redundant. <br/><br/>(3) Nonstatistical specifications place constraints on the item slection procedure.},
   pages = {12},
   title = {The impacts of computer adaptive testing from different perspectives},
   volume = {14},
   year = {2017},
}
