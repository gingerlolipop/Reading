   issue = {2},
   journal = {Pediatric Diabetes},
   keywords = {Administrative health data,Childhood diabetes,Disease surveillance},
   pages = {122-128},
   title = {Validation of a health administrative data algorithm for assessing the epidemiology of diabetes in Canadian children},
   volume = {11},
