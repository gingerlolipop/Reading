@article{Westland2007,
   abstract = {Partial least squares (PLS) statistical approaches were developed by Hermann Wold over three decades ago. More recently they have become popular in IS research for their ability to sort out perceived and hidden information systems artifacts – artifacts which are often latent constructs – in large, complex, multivariate datasets. Wold (1980, 1981) specifically advised that PLS was not suitable for confirmatory testing, rather should be used for prediction and the exploration of ‘plausible causality,’ though he did not further pursue his definition of ‘plausibility.’ Despite this, PLS is regularly employed for confirmatory testing in IS research, largely because it will converge to parameter estimates when maximum likelihood estimators cannot be found. This note provides a methodological commentary concerning the manner in which confirmatory PLS can be justified, providing that added information about the degree of plausibility is reported. PLS then provides a ‘middle way’ between confirmatory maximum likelihood methods such as LISREL-ML and the less restrictive exploratory model specification searches provided by factor analysis. The note extends existing methods by showing how to compute two plausibility indices to accompany research invoking confirmatory PLS. These will both locate specific research results in the continuum between purely confirmatory and purely exploratory testing, and help to specify required sample sizes for structural model fitting.},
   author = {J Christopher Westland},
   issue = {April},
   journal = {Decision Sciences},
   pages = {1-31},
   title = {Confirmatory Analysis with Partial Least Squares Confirmatory Analysis with Partial Least Squares},
   url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.117.3014&rep=rep1&type=pdf},
   year = {2007},
}
