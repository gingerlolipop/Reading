@article{Evans2015,
   abstract = {Background and purpose: Stereotactic body radiotherapy (SBRT) is being used for prostate cancer, but concerns persist about toxicity compared to other radiotherapy options. Materials and methods: We conducted a multi-institutional pooled cohort analysis of patient-reported quality of life (QOL) [EPIC-26] before and after intensity-modulated radiotherapy (IMRT), brachytherapy, or SBRT for localized prostate cancer. Data were analyzed by mean domain score, minimal clinically detectable difference (MCD) in domain score, and multivariate analyses to determine factors associated with domain scores at 2-years. Results: Data were analyzed from 803 patients at baseline and 645 at 2-years. Mean declines at 2-years across all patients were À1.9, À4.8, À4.9, and À13.3 points for urinary obstructive, urinary incontinence, bowel, and sexual symptom domains, respectively, corresponding to MCD in 29%, 20%, and 28% of patients. On multivariate analysis (vs. IMRT), brachytherapy had worse urinary irritation at 2-years (À6.8 points, p < 0.0001) but no differences in other domains (p > 0.15). QOL after SBRT was similar for urinary (p > 0.5) and sexual domains (p = 0.57), but was associated with better bowel score (+6.7 points, p < 0.0002). Conclusions: QOL 2-years after brachytherapy, IMRT, or SBRT is very good and largely similar, with small differences in urinary and bowel QOL that are likely minimized by modern techniques. Ó Prostate cancer radiation treatment options, including intensity-modulated (IMRT), brachytherapy implants, and recently stereotactic body radiotherapy (SBRT) each have relative advantages and disadvantages. All can have significant sexual, urinary, and bowel QOL impacts [1-4]. SBRT has presumed radiobiological advantages over IMRT [5]. Brachytherapy is completed in 1 day, SBRT in 1-3 weeks, and IMRT in 7-8 weeks. Single-institution SBRT trials show PSA control equivalent to IMRT or brachytherapy after short follow-up [6]. Cost-effectiveness analyses show SBRT is less expensive than IMRT [7], though brachytherapy is less expensive than both [8]. Convenience, non-invasiveness, and cost effectiveness are leading to increasing SBRT use nationally [9], though concerns about SBRT toxicity remain [10]. A recent phase I/II dose-escalation trial reported 9.8% grade 3-4 rectal toxicity at the highest dose level (50 Gy in 5 fractions) [11], and a retrospective study of Medicare claims data showed genitourinary (GU) complications and a transient GI toxicity were more common with SBRT [9]. In contrast, single-institutional studies evaluating QOL after prostate SBRT revealed transient declines in urinary QOL that resolved within 6 months after treatment while GI QOL was minimally affected, and sexual QOL appeared to decline similar to other radiotherapy modalities [12-16]. Here, we present a pooled analysis of QOL http://dx.},
   author = {Joseph R Evans and Shuang Zhao and Stephanie Daignault and Martin G Sanda and Jeff Michalski and Howard M Sandler and Deborah A Kuban and Jay Ciezki and Irving D Kaplan and Anthony L Zietman and Larry Hembroff and Felix Y Feng and Simeng Suy and Ted A Skolarus and Patrick W Mclaughlin and John T Wei and Rodney L Dunn and Steven E Finkelstein and Constantine A Mantz and Sean P Collins and Daniel A Hamstra},
   doi = {10.1016/j.radonc.2015.07.016},
   journal = {Radiotherapy and Oncology},
   keywords = {Brachytherapy,Prostate cancer,Quality of life,Stereotactic body radiotherapy (SBRT) IMRT},
   pages = {179-184},
   title = {Patient reported outcome measures Patient-reported quality of life after stereotactic body radiotherapy (SBRT), intensity modulated radiotherapy (IMRT), and brachytherapy},
   volume = {116},
   url = {http://dx.doi.org/10.1016/j.radonc.2015.07.016},
   year = {2015},
}
