   keywords = {Causal modeling,Confirmatory factor analysis,Covariance structure modeling,Data-model fit,Exploratory factor analysis,Factor analysis,Latent variable analysis,Maximum likelihood,Reliability,Structural equation modeling},
   pages = {686-690},
