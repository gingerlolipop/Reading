   keywords = {hematuria,overactive bladder,urinary bladder cancer},
   pages = {4292},
   title = {Urothelial bladder carcinoma with major clinical presentation as overactive bladder, without hematuria: case report and literature review},
   volume = {7},
   year = {2018},
}
