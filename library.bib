@report{,
   abstract = {Comparing NLP systems to select the best one for a task of interest, such as named entity recognition, is critical for practitioners and researchers. A rigorous approach involves setting up a hypothesis testing scenario using the performance of the systems on query documents. However, often the hypothesis testing approach needs to send a large number of document queries to the systems, which can be problematic. In this paper, we present an effective alternative based on the multi-armed bandit (MAB). We propose a hierarchical gen-erative model to represent the uncertainty in the performance measures of the competing systems, to be used by Thompson Sampling to solve the resulting MAB. Experimental results on both synthetic and real data show that our approach requires significantly fewer queries compared to the standard benchmarking technique to identify the best system according to F-measure.},
   author = {Gholamreza Haffari and Tuan Tran and Mark Carman},
   journal = {the Association for Computational Linguistics},
   note = {This paper seems to use old terms.<br/><br/>What is multi-armed bandit? It's like gambling machines. Select the best machine.<br/><br/>How to use this strategy in nlp? Now I only know two methods, doc2vec and the matrix svd method. Do I use multiple methods then choose the best? If so, since the testing sample are possibly biased (maybe misdiagnosed), won't the bias strengthened after the best arm selection (overfitting)?},
   pages = {408-416},
   title = {Efficient Benchmarking of NLP APIs using Multi-armed Bandits},
   volume = {1},
   url = {https://www.aclweb.org/anthology/E17-1039},
}
