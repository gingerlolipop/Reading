   keywords = {Chinese,Prostate cancer,quality of life,reliability,validity},
   note = {CFA EPIC-26<br/><br/>factorial validity -&gt; EPIC-26 and EPIC-CP, confirmatory fac- tory analysis for each of these instruments was per- formed using Mplus<br/><br/>Reliability -&gt; internal consistency<br/><br/>Overall internal consistency -&gt; Cronbach alpha<br/><br/>Domain internal consistency -&gt; Cronbach alpha<br/><br/>Stand alone item not included in any domain<br/><br/>Factorial Validity -&gt; Goodness-of-fit indicies of both <b>hierarchical</b> and correlated<b> five-factor model<br/></b>},
   pages = {1085-1090},
   publisher = {Elsevier Inc},
   title = {Psychometric Assessment of the Chinese Version of the Abbreviated Expanded Prostate Cancer Index Composite (EPIC-26) and the Clinical Practice Version (EPIC-CP) in Chinese Men With Prostate Cancer},
